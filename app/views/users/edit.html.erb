<h1>User Edit Form</h1>
<%= form_for(current_user, url: update_user_path(current_user), html: {method: "patch"}) do |f|%>
  <%=f.label :username%>
  <%=f.text_field :username%>
  </br>
  <%=f.label :email%>
  <%=f.text_field :email%>
  </br>
  <%=f.label :password%>
  <%=f.text_field :password%>
  </br>
  <%=f.label :profile%>
  <%=f.text_field :profile%>
  </br>
  <%=f.submit%>
  </br>
<%end%>